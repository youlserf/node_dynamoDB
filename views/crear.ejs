<%- include("template/cabecera", {tituloweb: 'Contactos' }) %>

    <div class="container">

        <h1>Crear nuevo contacto</h1>

        <form action="/contactos" class="my-2" method="POST">

            <input type="text" placeholder="Ingrese Nombre" class="form-control my-2" name="name">

            <input type="text" placeholder="Ingrese Email" class="form-control my-2" name="email">

            <input type="text" placeholder="Ingrese Apellidos" class="form-control my-2" name="last_name">

            <input type="text" placeholder="Ingrese Telefono" class="form-control my-2" name="phone">

            <button type="submit" class="btn btn-dark btn-block">
                Agregar
            </button>

        </form>

    </div>


    <%- include("template/footer") %>